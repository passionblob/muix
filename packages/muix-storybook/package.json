{
  "name": "@montehm/muix-storybook",
  "version": "0.0.1",
  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "watch-stories": "watch 'yarn run prestorybook' ./src/stories",
    "start-storybook-web": "node ./node_modules/@storybook/react/dist/server/index.js -p 2995 -c ./src/config_web",
    "storybook-web": "yarn run start-storybook-web",
    "prestorybook": "rnstl",
    "build:web": "yarn build-storybook -c ./src/config_web",
    "chromatic": "npx chromatic build:web --project-token cb998b5d9bfa",
    "deploy:web": "yarn chromatic -b build:web"
  },
  "dependencies": {
    "@monthem/muix": "*",
    "chroma-js": "^2.1.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-web": "^0.15.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react": "^6.1.20",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/chroma-js": "^2.1.3",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.7",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-native-web": "^0.15.0",
    "chromatic": "^5.6.3",
    "eslint": "^6.5.1",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-docgen-typescript": "^1.21.0",
    "react-dom": "^17.0.1",
    "react-native-storybook-loader": "^2.0.2",
    "react-test-renderer": "16.13.1",
    "ts-loader": "^8.0.17",
    "typescript": "^3.8.3",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "@types/react": "^16"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/stories"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./src/storyLoader.js"
    }
  }
}
